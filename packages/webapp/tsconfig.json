{
  "compilerOptions": {
    "sourceMap": true,
    "strict": true,
    "module": "es6",
    "jsx": "react",
    "target": "es5",
    "lib": ["dom", "es2017", "ESNext"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true
  },
  "include": ["src", "*.ts", "*.js"],
  "exclude": ["node_modules"]
}
