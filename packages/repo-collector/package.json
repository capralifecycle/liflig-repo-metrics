{
  "name": "@liflig/repo-metrics-repo-collector",
  "version": "0.0.0-development",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc -noEmit && rm -rf dist && esbuild src/lambda.ts --bundle --platform=node --target=\"node18.0\" --outfile=dist/index.js",
    "test": "vitest --run",
    "collect-locally": "tsx src/collect-locally.ts",
    "aggregate-locally": "tsx src/aggregate-locally.ts",
    "report-locally": "tsx src/report-locally.ts",
    "serve": "http-server --cors -p 8383 data"
  },
  "devDependencies": {
    "@aws-sdk/client-cloudfront": "3.953.0",
    "@aws-sdk/client-s3": "3.953.0",
    "@aws-sdk/client-secrets-manager": "3.953.0",
    "@octokit/types": "16.0.0",
    "@types/aws-lambda": "8.10.160",
    "@types/js-yaml": "4.0.9",
    "@types/lodash-es": "4.17.12",
    "@types/node": "24.10.12",
    "esbuild": "0.27.3",
    "http-server": "14.1.1",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "vitest": "4.0.18"
  },
  "dependencies": {
    "@js-temporal/polyfill": "0.5.1",
    "@liflig/repo-metrics-repo-collector-types": "0.0.0-development",
    "@octokit/rest": "^22.0.1",
    "ajv": "8.17.1",
    "axios": "1.13.2",
    "date-holidays": "3.26.8",
    "js-yaml": "4.1.1",
    "lodash-es": "4.17.23",
    "p-limit": "^7.2.0"
  }
}
