{
  "name": "@liflig/repo-metrics-repo-collector",
  "version": "0.0.0-development",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc -noEmit && rm -rf dist && esbuild src/lambda.ts --bundle --alias:keytar=./keytar-stub.cjs --platform=node --target=\"node18.0\" --outfile=dist/index.js",
    "test": "vitest --run",
    "lint": "biome check",
    "lint:fix": "biome check --fix",
    "collect-locally": "tsx src/collect-locally.ts",
    "aggregate-locally": "tsx src/aggregate-locally.ts",
    "report-locally": "tsx src/report-locally.ts",
    "serve": "http-server --cors -p 8383 data"
  },
  "devDependencies": {
    "@aws-sdk/client-cloudfront": "3.830.0",
    "@aws-sdk/client-s3": "3.832.0",
    "@aws-sdk/client-secrets-manager": "3.830.0",
    "@octokit/types": "14.1.0",
    "@types/aws-lambda": "8.10.150",
    "@types/js-yaml": "4.0.9",
    "@types/lodash-es": "4.17.12",
    "@types/node": "22.15.32",
    "@types/node-fetch": "2.6.12",
    "esbuild": "0.25.5",
    "http-server": "14.1.1",
    "tsx": "4.20.3",
    "typescript": "5.8.3"
  },
  "dependencies": {
    "@js-temporal/polyfill": "0.5.1",
    "@liflig/repo-metrics-repo-collector-types": "0.0.0-development",
    "@octokit/rest": "^22.0.0",
    "@types/lodash": "4.17.18",
    "@vitest/ui": "3.2.4",
    "ajv": "8.17.1",
    "axios": "1.10.0",
    "cachedir": "^2.4.0",
    "date-holidays": "3.24.3",
    "get-stream": "9.0.1",
    "js-yaml": "4.1.0",
    "keytar": "7.9.0",
    "lodash-es": "4.17.21",
    "node-fetch": "^3.0.0",
    "p-limit": "^6.0.0",
    "vitest": "3.2.4"
  }
}
