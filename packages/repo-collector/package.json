    "@octokit/rest": "^21.0.0",
    "@types/lodash": "4.17.17",
    "@vitest/ui": "3.1.4",
    "ajv": "8.17.1",
    "axios": "1.9.0",
    "cachedir": "^2.4.0",
    "date-holidays": "3.24.3",
    "get-stream": "9.0.1",
    "js-yaml": "4.1.0",
    "keytar": "7.9.0",
    "lodash-es": "4.17.21",
    "node-fetch": "^3.0.0",
    "p-limit": "^6.0.0",
    "vitest": "3.1.4"
  }
