    "aggregate-locally": "tsx src/aggregate-locally.ts",
    "report-locally": "tsx src/report-locally.ts",
    "serve": "http-server --cors -p 8383 data"
  },
  "devDependencies": {
    "@aws-sdk/client-cloudfront": "3.723.0",
    "@aws-sdk/client-s3": "3.723.0",
